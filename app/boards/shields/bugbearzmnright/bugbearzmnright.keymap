#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp RPAR  &kp T  &kp J  &kp Q  &kp U  &kp BSPC
                &kp E     &kp O  &kp Y  &kp G  &kp F  &kp DEL
                &kp QMARK &kp A  &kp I  &kp K  &kp M  &kp RETURN
                &kp X     &kp X  &kp LEFT &kp RIGHT &kp X &kp X
                
                &kp SPACE &kp COMMA &kp DOT
                &mo 1 &mo 2 &kp PRINTSCREEN

            >;
        };
        layer_1 {
            bindings = <
                &kp EQUAL   &kp N7  &kp N8   &kp N9  &kp SLASH  &kp BSPC
                &kp X       &kp N4  &kp N5   &kp N6  &kp BACKSLASH  &kp DEL
                &kp X      &kp N1  &kp N2   &kp N3  &kp MINUS  &kp RETURN
                &kp X      &kp X  &kp LBKT &kp RBKT &kp X &kp X

                &kp SPACE &kp COMMA &kp DOT
                &mo 1 &mo 2 &kp PRINTSCREEN
            >;
       };
    };
};
