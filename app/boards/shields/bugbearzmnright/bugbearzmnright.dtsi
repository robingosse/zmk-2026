/ {
    model = "bugbearzmnright";
    compatible = "bugbearzmnright";

    chosen {
        zmk,physical-layout = &physical_layout0;
    };

    physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        keys = <
            &k0  0 0  1900 1900    // Key 1 at 0,0
            &k1 1900 0 1900 1900   // Key 2 at 19mm,0
            // ... You must write this line for EVERY key ...
        >;
    };
};
